{
  "version": 2,
  "builds": [
    {
      "src": "front/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "front/dist/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "https://hgcj-back.vercel.app"
  }
}




